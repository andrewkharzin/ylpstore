{
    "name": "andrewkharzin/shop",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "~5|~6|~7|~8",
        "mll-lab/graphql-php-scalars": "^4",
        "nuwave/lighthouse": "^5.12.1",
        "laravel/legacy-factories": "^1",
        "cviebrock/eloquent-sluggable": "^8.0",
        "laravel/sanctum": "^2.7",
        "mll-lab/laravel-graphql-playground": "^2.1",
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-medialibrary": "^9.4.0",
        "spatie/laravel-permission": "^3.11",
        "php-http/guzzle7-adapter": "^0.1.1",
        "bensampo/laravel-enum": "^3.1.0",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },    
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andrewkharzin\\Shop\\": "src/"
        },
         "classmap": [
            "src"
        ]
    },
        "autoload-dev": {
        "psr-4": {
            "Andrewkhrzin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrewkharzin\\ShopServiceProvider"
            ],
            "aliases": {
                "Shop": "Andrewkharzin\\Facades\\Shop"
            }
        }
    },
    "minimum-stability": "dev"
    
}
